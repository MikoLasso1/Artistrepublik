{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-prototype-builtins": "off",
    "no-undef": "off",
    "no-useless-escape": "off",
    "vue/no-side-effects-in-computed-properties": "off"
  }
}
